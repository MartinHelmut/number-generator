{
  "name": "number-generator",
  "version": "2.2.1",
  "description": "Generate pseudo random numbers and hashs.",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "shx rm -fr ./lib/* && rollup -c rollup.config.js && shx cp ./src/*.d.ts lib/",
    "build:docs": "jsdoc -c .jsdoc.json --verbose",
    "test": "jest",
    "test:prod": "npm run build && cross-env NODE_ENV=production jest",
    "test:treeshaking": "agadoo lib/index.esm.js",
    "ci": "npm run lint && npm run test:prod",
    "test:coverage": "jest && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src/",
    "format": "prettier --write \"**/*.{js,md}\"",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "prerelease": "npm run ci",
    "release": "standard-version -a",
    "prepare": "npm run ci"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MartinHelmut/number-generator.git"
  },
  "keywords": [
    "numbers",
    "random",
    "alea",
    "murmur",
    "hash",
    "generator",
    "prng"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinHelmut/number-generator/issues"
  },
  "homepage": "https://github.com/MartinHelmut/number-generator#readme",
  "devDependencies": {
    "agadoo": "1.0.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.7.0",
    "commitizen": "2.10.1",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.6.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-compat": "2.5.1",
    "husky": "1.0.0",
    "jest": "23.6.0",
    "jsdoc": "3.5.5",
    "lint-staged": "7.3.0",
    "prettier": "1.14.3",
    "rollup": "0.66.2",
    "rollup-plugin-babel": "3.0.7",
    "rollup-plugin-closure-compiler-js": "1.0.6",
    "rollup-plugin-filesize": "4.0.1",
    "rollup-plugin-node-resolve": "3.4.0",
    "shx": "0.3.2",
    "standard-version": "4.4.0"
  },
  "files": [
    "lib"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 4,
    "singleQuote": true
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "browserslist": [
    "> 1%",
    "Last 2 versions",
    "IE >= 9"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "browser": true,
      "jest": true
    },
    "plugins": [
      "compat"
    ],
    "rules": {
      "compat/compat": "error"
    }
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "dependencies": {
      "semanticPrefix": "chore:",
      "semanticCommitScope": ""
    },
    "pin": {
      "semanticPrefix": "chore:",
      "semanticCommitScope": ""
    },
    "semanticPrefix": "chore:",
    "semanticCommitScope": ""
  }
}
