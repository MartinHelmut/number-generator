{
  "name": "number-generator",
  "version": "1.0.3",
  "description": "Generate pseudo random numbers and hashs.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "postbuild": "uglifyjs ./lib/index.js --screw-ie8 -m toplevel -r 'require,exports' -c -o ./lib/index.js --lint && cp ./src/index.d.ts lib/",
    "build": "rimraf ./lib/* && babel src -d lib",
    "build:docs": "jsdoc src/index.js -r -R ./README.md -d docs",
    "pretest": "npm run lint && npm run build",
    "test": "jest --coverage",
    "test:coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint --ext .js src/",
    "format": "prettier --print-width 80 --tab-width 4 --single-quote --write \"src/**/*.js\"",
    "commit": "git-cz",
    "release": "standard-version -a",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MartinHelmut/number-generator.git"
  },
  "keywords": [
    "numbers",
    "random",
    "alea",
    "murmur",
    "hash",
    "generator"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinHelmut/number-generator/issues"
  },
  "homepage": "https://github.com/MartinHelmut/number-generator#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.6.1",
    "commitizen": "2.9.6",
    "coveralls": "3.0.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint-plugin-compat": "2.2.0",
    "eslint": "4.17.0",
    "jest": "22.2.1",
    "jsdoc": "3.5.5",
    "prettier": "1.10.2",
    "rimraf": "2.6.2",
    "standard-version": "4.2.0",
    "uglify-js": "3.3.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ]
  }
}
