{
  "name": "number-generator",
  "version": "3.0.1",
  "description": "Generate pseudo random numbers and hashs.",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:docs": "jsdoc -c jsdoc.config.js --verbose",
    "test": "nyc mocha --require @babel/register src/__tests__/*.js src/**/__tests__/*.js",
    "test:prod": "npm run build && cross-env NODE_ENV=production nyc mocha --require @babel/register src/**/__tests__/*.js && mocha --require ts-node/register src/**/*.test.ts",
    "test:treeshaking": "agadoo lib/index.esm.js",
    "ci": "npm run format:check && npm run lint && npm run test:prod && npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --ignore-path .prettierignore ./",
    "format": "prettier --write \"**/*.{js,ts,md,json,yml}\"",
    "format:check": "prettier --list-different \"**/*.{js,ts,md,json,yml}\"",
    "commit": "git-cz",
    "prerelease": "npm run ci",
    "release": "standard-version -a",
    "prepare": "npm run ci"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MartinHelmut/number-generator.git"
  },
  "keywords": [
    "numbers",
    "random",
    "alea",
    "murmur",
    "hash",
    "generator",
    "prng"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinHelmut/number-generator/issues"
  },
  "homepage": "https://github.com/MartinHelmut/number-generator#readme",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/register": "7.5.5",
    "@types/chai": "4.2.0",
    "@types/mocha": "5.2.7",
    "agadoo": "1.0.1",
    "chai": "4.2.0",
    "chalk": "2.4.2",
    "commitizen": "4.0.3",
    "coveralls": "3.0.6",
    "cpy": "7.3.0",
    "cross-env": "5.2.0",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-compat": "3.2.0",
    "jsdoc": "3.6.3",
    "mocha": "6.2.0",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "regenerator-runtime": "0.13.3",
    "rollup": "1.20.2",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-clear": "2.0.7",
    "rollup-plugin-filesize": "6.2.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.1.1",
    "standard-version": "7.0.0",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  }
}
