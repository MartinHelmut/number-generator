{
  "name": "number-generator",
  "version": "4.0.5",
  "description": "Generate repeatable pseudo random numbers and non-cryptographic hash numbers for usage in Node.js and browser environments.",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:reference:code": "./scripts/generate-reference.sh",
    "build:tests": "rollup -c rollup-tests.config.js",
    "ci": "pnpm run test:prod && pnpm test",
    "commit": "git-cz",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "format": "prettier --write .",
    "format:check": "prettier --list-different .",
    "lint": "eslint --ignore-path .prettierignore ./",
    "prerelease": "pnpm run ci",
    "release": "standard-version -a",
    "docs": "jsdoc -c jsdoc.config.js --verbose && serve ./docs",
    "test": "nyc mocha --require @babel/register src/__tests__/*.test.js src/**/__tests__/*.test.js",
    "test:benchmark": "node --require @babel/register src/__tests__/murmur.benchmark.js && node --require @babel/register src/__tests__/alea.benchmark.js",
    "test:prod": "pnpm run build && NODE_ENV=production nyc mocha --require @babel/register src/**/__tests__/*.js && mocha --require ts-node/register src/**/*.test.ts",
    "test:treeshaking": "agadoo lib/index.esm.js",
    "prepare": "node ./scripts/ensure-package-manager.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MartinHelmut/number-generator.git"
  },
  "keywords": [
    "numbers",
    "random",
    "alea",
    "murmur",
    "hash",
    "generator",
    "prng"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.se>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinHelmut/number-generator/issues"
  },
  "homepage": "https://github.com/MartinHelmut/number-generator#readme",
  "files": [
    "lib"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "Last 2 versions",
    "IE >= 10",
    "node >= 8"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "browser": true,
      "mocha": true,
      "es6": true
    }
  },
  "nyc": {
    "reporter": [
      "html",
      "text"
    ]
  },
  "babel": {
    "exclude": "node_modules/**",
    "presets": [
      "@babel/preset-env"
    ]
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "baseBranches": [
      "main"
    ],
    "supportPolicy": [
      "lts_latest"
    ],
    "dependencies": {
      "semanticPrefix": "chore:",
      "semanticCommitScope": ""
    },
    "pin": {
      "semanticPrefix": "chore:",
      "semanticCommitScope": ""
    },
    "semanticPrefix": "chore:",
    "semanticCommitScope": "",
    "packageRules": [
      {
        "updateTypes": [
          "minor",
          "patch",
          "pin",
          "digest"
        ],
        "automerge": true
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/register": "7.12.10",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "11.0.1",
    "@rollup/plugin-replace": "2.3.4",
    "@types/chai": "4.2.14",
    "@types/mocha": "8.2.0",
    "@types/node": "12.19.11",
    "agadoo": "2.0.0",
    "benchmark": "2.1.4",
    "chai": "4.2.0",
    "commitizen": "4.2.2",
    "conventional-changelog": "3.1.24",
    "conventional-changelog-conventionalcommits": "4.5.0",
    "coveralls": "3.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "jsdoc": "3.6.6",
    "mocha": "8.2.1",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "rollup": "2.35.1",
    "rollup-plugin-clear": "2.0.7",
    "rollup-plugin-copy": "3.3.0",
    "rollup-plugin-external-globals": "0.6.1",
    "rollup-plugin-filesize": "9.1.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-terser": "7.0.2",
    "serve": "11.3.2",
    "standard-version": "9.0.0",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  }
}
