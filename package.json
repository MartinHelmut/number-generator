{
  "name": "number-generator",
  "version": "2.0.2",
  "description": "Generate pseudo random numbers and hashs.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "prebuild": "shx rm -fr ./lib/*",
    "build": "babel src -d lib --source-maps && rollup -c rollup.config.js && shx cp ./src/*.d.ts lib/",
    "build:docs": "jsdoc -c .jsdoc.json --verbose",
    "pretest": "npm run lint && npm run build",
    "test": "jest",
    "test:coverage": "jest && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint --ext .js src/",
    "format": "prettier --write \"**/*.{js,md}\"",
    "commit": "git-cz",
    "precommit": "lint-staged",
    "release": "standard-version -a",
    "prepare": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MartinHelmut/number-generator.git"
  },
  "keywords": [
    "numbers",
    "random",
    "alea",
    "murmur",
    "hash",
    "generator",
    "prng"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinHelmut/number-generator/issues"
  },
  "homepage": "https://github.com/MartinHelmut/number-generator#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.6.1",
    "commitizen": "2.9.6",
    "coveralls": "3.0.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.17.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-compat": "2.2.0",
    "husky": "0.14.3",
    "jest": "22.2.2",
    "jsdoc": "3.5.5",
    "lint-staged": "6.1.0",
    "prettier": "1.10.2",
    "rollup": "0.55.5",
    "rollup-plugin-babel": "3.0.3",
    "rollup-plugin-node-resolve": "3.0.2",
    "shx": "0.2.2",
    "standard-version": "4.3.0"
  },
  "files": [
    "lib",
    "!lib/__tests__"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 4,
    "singleQuote": true
  },
  "jest": {
    "roots": [
      "<rootDir>/lib"
    ],
    "collectCoverage": true
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "browserslist": [
    "> 1%",
    "Last 2 versions",
    "IE >= 9"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "browser": true,
      "jest": true
    },
    "plugins": [
      "compat"
    ],
    "rules": {
      "compat/compat": "error"
    }
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "dependencies": {
      "semanticPrefix": "chore:",
      "semanticCommitScope": ""
    },
    "pin": {
      "semanticPrefix": "chore:",
      "semanticCommitScope": ""
    },
    "semanticPrefix": "chore:",
    "semanticCommitScope": ""
  }
}
