{
  "name": "number-generator",
  "version": "4.0.1",
  "description": "Generate repeatable pseudo random numbers and non-cryptographic hash numbers for usage in Node.js and browser environments.",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:docs": "jsdoc -c jsdoc.config.js --verbose",
    "build:reference:code": "./scripts/generate-reference.sh",
    "build:tests": "rollup -c rollup-tests.config.js",
    "ci": "pnpm run format:check && pnpm run lint && pnpm run test:prod && pnpm test",
    "commit": "git-cz",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "format": "prettier --write \"**/*.{js,ts,md,json,yml}\"",
    "format:check": "prettier --list-different \"**/*.{js,ts,md,json,yml}\"",
    "lint": "eslint --ignore-path .prettierignore ./",
    "preinstall": "node ./scripts/ensure-package-manager.js",
    "prepare": "pnpm run ci",
    "prerelease": "pnpm run ci",
    "release": "standard-version -a",
    "start:docs": "pnpm run build:docs && serve ./docs",
    "test": "nyc mocha --require @babel/register src/__tests__/*.test.js src/**/__tests__/*.test.js",
    "test:benchmark": "node --require @babel/register src/__tests__/murmur.benchmark.js && node --require @babel/register src/__tests__/alea.benchmark.js",
    "test:prod": "pnpm run build && cross-env NODE_ENV=production nyc mocha --require @babel/register src/**/__tests__/*.js && mocha --require ts-node/register src/**/*.test.ts",
    "test:treeshaking": "agadoo lib/index.esm.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MartinHelmut/number-generator.git"
  },
  "keywords": [
    "numbers",
    "random",
    "alea",
    "murmur",
    "hash",
    "generator",
    "prng"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinHelmut/number-generator/issues"
  },
  "homepage": "https://github.com/MartinHelmut/number-generator#readme",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@babel/preset-env": "7.7.1",
    "@babel/register": "7.7.0",
    "@types/chai": "4.2.4",
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.6",
    "agadoo": "1.1.0",
    "benchmark": "2.1.4",
    "chai": "4.2.0",
    "chalk": "3.0.0",
    "commitizen": "4.0.3",
    "conventional-changelog": "3.1.12",
    "conventional-changelog-conventionalcommits": "4.2.1",
    "coveralls": "3.0.7",
    "cpy": "7.3.0",
    "cross-env": "6.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "jsdoc": "3.6.3",
    "mocha": "6.2.2",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "regenerator-runtime": "0.13.3",
    "rollup": "1.26.5",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-clear": "2.0.7",
    "rollup-plugin-copy": "3.1.0",
    "rollup-plugin-external-globals": "0.4.0",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-terser": "5.1.2",
    "serve": "11.2.0",
    "standard-version": "7.0.0",
    "ts-node": "8.5.0",
    "typescript": "3.7.2"
  }
}
