{
  "name": "number-generator",
  "version": "0.2.16",
  "description": "Generate pseudo random numbers and hashs.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "postbuild": "uglifyjs ./lib/index.js --screw-ie8 -m toplevel -r 'require,exports' -c -o ./lib/index.js --lint && cp ./src/index.d.ts lib/",
    "build": "rimraf ./lib/* && babel src -d lib",
    "build:docs": "jsdoc src/index.js -r -R ./README.md -d docs",
    "pretest": "npm run lint && npm run build",
    "test": "nyc mocha --recursive ./lib/**/*-test.js",
    "test:coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:browser": "karma start karma.config.js",
    "test:browser:sauce": "karma start karma-sauce.config.js",
    "lint": "eslint src/**/*.js",
    "format": "prettier --print-width 80 --tab-width 4 --single-quote --write \"src/**/*.js\"",
    "commit": "git-cz",
    "release": "standard-version -a",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MartinHelmut/number-generator.git"
  },
  "keywords": [
    "numbers",
    "random",
    "alea",
    "murmur",
    "hash",
    "generator"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinHelmut/number-generator/issues"
  },
  "homepage": "https://github.com/MartinHelmut/number-generator#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babelify": "8.0.0",
    "browserify": "14.5.0",
    "chai": "4.1.2",
    "commitizen": "2.9.6",
    "coveralls": "3.0.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.10.0",
    "eslint-plugin-compat": "2.0.1",
    "jsdoc": "3.5.5",
    "karma": "1.7.1",
    "karma-browserify": "5.1.1",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "1.0.1",
    "karma-mocha": "1.3.0",
    "karma-safari-launcher": "1.0.0",
    "karma-sauce-launcher": "1.2.0",
    "mocha": "4.0.1",
    "nyc": "11.3.0",
    "prettier": "1.7.4",
    "rimraf": "2.6.2",
    "standard-version": "4.2.0",
    "uglify-js": "3.1.6"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
